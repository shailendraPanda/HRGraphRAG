#    Classes

:Certification rdf:type owl:Class ;
rdfs:label "Certification"@en ;
rdfs:comment "Professional certifications held by candidates"@en .

:Company rdf:type owl:Class ;
rdfs:label "Company"@en ;
rdfs:comment "Organizations offering job positions"@en .

:HRDocument rdf:type owl:Class ;
rdfs:label "HR Document"@en ;
rdfs:comment "Base class for all HR-related documents"@en .

:Job rdf:type owl:Class ;
rdfs:label "Job"@en ;
rdfs:comment "Job positions offered by companies"@en .

:JobDescription rdf:type owl:Class ;
rdfs:subClassOf :HRDocument ;
rdfs:label "Job Description"@en ;
rdfs:comment "Documents describing job requirements and details"@en .

:Location rdf:type owl:Class ;
rdfs:label "Location"@en ;
rdfs:comment "Geographic locations"@en .

:Person rdf:type owl:Class ;
rdfs:subClassOf foaf:Person ;
rdfs:label "Person"@en ;
rdfs:comment "Job candidates represented as persons"@en .

:Resume rdf:type owl:Class ;
rdfs:subClassOf :HRDocument ;
rdfs:label "Resume"@en ;
rdfs:comment "Candidate resume documents"@en .

:Role rdf:type owl:Class ;
rdfs:label "Role"@en ;
rdfs:comment "Professional roles or job functions"@en .

:Skill rdf:type owl:Class ;
rdfs:label "Skill"@en ;
rdfs:comment "Professional skills and competencies"@en .

:WorkExperience rdf:type owl:Class ;
rdfs:label "Work Experience"@en ;
rdfs:comment "Professional work experience records"@en .