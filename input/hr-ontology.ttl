@prefix : <http://example.com/hr#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@base <http://example.com/hr> .

<http://example.com/hr> rdf:type owl:Ontology ;
rdfs:label "HR Domain Ontology"@en ;
rdfs:comment "Ontology for Human Resources domain covering resumes and job descriptions"@en .


# Object Properties

:atCompany rdf:type owl:ObjectProperty ;
     rdfs:domain :Job ;
     rdfs:range :Company ;
     rdfs:label "at company"@en ;
     rdfs:comment "Relates a job to the company offering it"@en .

:hasCertification rdf:type owl:ObjectProperty ;
rdfs:domain :Person ;
rdfs:range :Certification ;
rdfs:label "has certification"@en ;
rdfs:comment "Relates a person to their certifications"@en .

:hasLocationMatchWith rdf:type owl:ObjectProperty ;
rdfs:domain :Person ;
rdfs:range :Job ;
rdfs:label "has location match with"@en ;
rdfs:comment "Indicates location compatibility between candidate and job"@en .

:hasMatchingSkillWith rdf:type owl:ObjectProperty ;
rdfs:domain :Person ;
rdfs:range :Job ;
rdfs:label "has matching skill with"@en ;
rdfs:comment "Indicates skill match between candidate and job"@en .

:hasRole rdf:type owl:ObjectProperty ;
rdfs:domain :WorkExperience ;
rdfs:range :Role ;
rdfs:label "has role"@en ;
rdfs:comment "Relates work experience to a role"@en .

:hasSalaryMatchWith rdf:type owl:ObjectProperty ;
rdfs:domain :Person ;
rdfs:range :Job ;
rdfs:label "has salary match with"@en ;
rdfs:comment "Indicates salary compatibility between candidate and job"@en .

:hasSkill rdf:type owl:ObjectProperty ;
rdfs:domain :Person ;
rdfs:range :Skill ;
rdfs:label "has skill"@en ;
rdfs:comment "Relates a person to their skills"@en .

:hasWorkExperience rdf:type owl:ObjectProperty ;
rdfs:domain :Person ;
rdfs:range :WorkExperience ;
rdfs:label "has work experience"@en ;
rdfs:comment "Relates a person to their work experience"@en .

:livesIn rdf:type owl:ObjectProperty ;
rdfs:domain :Person ;
rdfs:range :Location ;
rdfs:label "lives in"@en ;
rdfs:comment "Relates a person to their location"@en .

:locatedAt rdf:type owl:ObjectProperty ;
rdfs:domain owl:Thing ;
rdfs:range :Location ;
rdfs:label "located at"@en ;
rdfs:comment "Relates an entity to a location"@en .

:requiresRole rdf:type owl:ObjectProperty ;
rdfs:domain :Job ;
rdfs:range :Role ;
rdfs:label "requires role"@en ;
rdfs:comment "Relates a job to required roles"@en .

:requiresSkill rdf:type owl:ObjectProperty ;
rdfs:domain :Job ;
rdfs:range :Skill ;
rdfs:label "requires skill"@en ;
rdfs:comment "Relates a job to required skills"@en .

:usedSkill rdf:type owl:ObjectProperty ;
rdfs:domain :WorkExperience ;
rdfs:range :Skill ;
rdfs:label "used skill"@en ;
rdfs:comment "Relates work experience to skills used"@en .


